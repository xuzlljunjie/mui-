/* 顶部导航*/
.header_box{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 42px;
	padding: 0 13px;
	line-height: 42px;
	color: #ffffff;
	font-size: 16px;
	text-align: center;
	background: #00afd5;	
	box-sizing: border-box;
	z-index: 1;
}
.sigin_header h2,
.header_box h2{
	font-size: 16px;
	height: 42px;
	line-height: 42px;
	font-weight: normal;
	text-align: center;
	width: calc(100% - 96px);
	margin-left: 48px;
}
.header_box span{
	position: absolute;
	top: 0;
	right: 0;
	display: block;
	width: 50px;
	height: 44px;
}
.header_box span:active{
	opacity: 0.84;
}
.header_box .icon_back{
	left: 0px;
	background: url(../../img/image/icon_header.png) no-repeat 12px 0;
	background-size: 100% auto;
}
.header_box .icon_finish{
	background: url(../../img/image/icon_header.png) no-repeat 0 -44px;
	background-size: 100% auto;
}
.header_box .icon_edit{
	background: url(../../img/image/icon_header.png) no-repeat 0 -88px;
	background-size: 100% auto;
}
.icon_tex{
	font-size: 14px;
	color: rgba(255,255,255,0.87);
}
.icon_screening{
	position: absolute;
	top: 0;
	right: 0;
	font-size: 13px;
	padding-right: 38px;
	background: url(../../img/image/icon_screening.png) no-repeat right center;
	background-size: auto 100%;	
}
/* 设置密码 */
.set_password{
	position: relative;
	width: 100%;
	height: 42px;
	padding: 0 12px;
	font-size: 0;
	border-bottom:1px solid #e5e5e7;
	border-top:1px solid #e5e5e7;
	background: #fff;	
	box-sizing: border-box;
}
.set_password h2{
	display: inline-block;
	font-size: 15px;
	line-height: 42px;
	color: #212121;
	padding-right: 18px;
}
.set_password input{
	width: 60%;
	height: 40px;
	/*line-height: 40px;*/
	font-size: 14px;
	color: #757575;
	border: none;
	background: none;
}
/* placeholder 样式设置  */
.color_placeholder_bdbdbd:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	color: #bdbdbd;
}
.color_placeholder_bdbdbd::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	color: #bdbdbd;
}
.color_placeholder_bdbdbd:-ms-input-placeholder{
	color: #bdbdbd;
}
.color_placeholder_bdbdbd::-webkit-input-placeholder{
	color: #bdbdbd;
	text-decoration: ;
}
/* placeholder 字体设置  */
.placeholder_font_13_bdbdbd:-moz-placeholder {
	/* Mozilla Firefox 4 to 18 */
	color: #bdbdbd;
	font-size: 13px;
}
.placeholder_font_13_bdbdbd::-moz-placeholder {
	/* Mozilla Firefox 19+ */
	color: #bdbdbd;	
	font-size: 13px;
}
.placeholder_font_13_bdbdbd:-ms-input-placeholder{
	color: #bdbdbd;	
	font-size: 13px;
}
.placeholder_font_13_bdbdbd::-webkit-input-placeholder{
	color: #bdbdbd;	
	font-size: 13px;
	/*text-decoration: ;*/
}
.set_password .send_code{
	position: absolute;
	top: 12px;
	right: 12px;
