/* 上传头像弹窗  */
.popup_window_camera{
	position: fixed;
	top: 0;
	left: 0;
	width: 100%;
	height: 100%;
	background: rgba(0,0,0,0.4);
	z-index: 1;
}
.popup_window_bottom{
	position: fixed;
	bottom: 0;
	left: 0;
	width: 100%;
	background: #f5f5f9;
	border-radius: 5px 5px 0 0;
	border-top: 1px solid #e5e5e7;
}
.popup_window_bottom .operation_box{
	height: 36px;
	border-bottom: 1px solid #e5e5e7;
}
.icon_close{
	float: left;
	padding: 12px;
	display: block;
	width:36px;
	height: 36px;
	background: url(../../img/image/icon_close.png) no-repeat center center;
	background-size: 40%;
}
.operation_box .icon_finish{
	margin-top: -1px;
	float: right;
	padding: 12px;
	display: block;
	width:36px;
	height: 36px;
	background: url(../../img/image/icon_finish.png) no-repeat center center;
	background-size: 52%;
}
.icon_close:active{
	opacity: 0.86;
}
.popup_window_bottom .camera_box{
	width: 100%;
	overflow: hidden;
}
.popup_window_bottom .camera_box li{
	display: table-cell;
	width: 1%;
