    font-size: 0;
    text-align: center;
}
.popup_window_bottom .camera_box li:first-child{ padding-left: 10%;}
.popup_window_bottom .camera_box li:last-child{ padding-right: 10%;}
.popup_window_bottom .camera_box li p{
	vertical-align: top;
	padding-top: 52px;
	display: inline-block;
	width: 88px;
	color: #757575;
	font-size: 10px;
	text-align: center;
}
.popup_window_bottom .camera_box .icon_camera{
	background:url(../../img/image/icon_popout_camera.png) no-repeat center 1px;
	background-size: 50% auto;
}
.popup_window_bottom .camera_box .icon_poto{
	background:url(../../img/image/icon_popout_camera.png) no-repeat center -82px;
	background-size: 50% auto;
}
.popup_window_bottom .camera_box .icon_vedio{
	background: url(../../img/image/icon_popout_camera.png) no-repeat center -165px;
	background-size: 50% auto;
}
.popup_window_bottom .camera_box .video_display {
	background: url(../../img/image/icon_popout_camera.png) no-repeat center -249px;
	background-size: 50% auto;
}

